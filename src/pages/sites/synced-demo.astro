---
import Layout from "@layouts/Layout.astro";
import BlankSite from "@components/BlankSite/BlankSite";
import { Store } from "@tomic/react";

const store = new Store({
  serverUrl: "https://atomicdata.dev",
});

const resource = await store.fetchResourceFromServer(
  "https://atomicdata.dev/module-config/tlpiqvfbd2e"
);

const configJson = resource.get(
  "https://atomicdata.dev/property/pq0cvnocfr"
) as string;
---

<Layout title="Synced to atomic server" favicon="" themeColor=""
  ><BlankSite client:load config={configJson} />
</Layout>
