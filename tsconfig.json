{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": "./",
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "esModuleInterop": true,
    "paths": {
      "@/*": ["./src/*"],
      "@@/*": ["./*"],
      "@lib/*": ["src/lib/*"]
    }
  },
  "ts-node": {
    "esm": true
  },
}